
<%= render 'layouts/partials/header/header_contact' %>

<main>
    <section class="contact">
        <div class="container">
            <div class="row">
                <div class="col-12 wow fadeIn">
                    <div class="title">
                        <h5>CONTACTO</h5>
                        <h3>Vamos conversar</h3>
                    </div>
                    <!--end title --> 
                </div>
                <!-- end col-12 -->
                <div class="col-md-6 wow fadeIn">
                    <div class="contact-box">
                        <h6>NOSSO ENDEREÇO</h6>
                        <p>
                            <%= setting.street %>
                            <br>
                            <%= setting.address %>
                        </p>
                    </div>
                    <!--end contact-box -->
                </div>
                <!-- end col-6 -->
                <div class="col-md-6 wow fadeIn" data-wow-delay="0.1s">
                    <div class="contact-box">
                        <h6>lIGA PARA NÓS</h6>
                        <a href="#"><%= setting.cell_phone1 %></a>
                        <a href="#"><%= setting.cell_phone2 %></a>
                        <h6>E-MAIL</h6>
                        <a href="#"><%= setting.email %></a> 
                    </div>
                    <!--end contact-box --> 
                </div>
                <!-- end col-6 -->
                <div class="col-12 wow fadeIn">
                    <div class="map" id="map"></div>
                    <!-- end map --> 
                </div>
                <!-- end col-12 -->
                <div class="col-md-5 wow fadeIn">
                    <div class="title">
                        <h5>FORMULÁRIO DE CONTACTO</h5>
                        <h3>Se houver alguma questão Por favor</h3>
                    </div>
                    <!--end title -->
                </div>
                <!-- end col-5 -->
                <div class="col-md-6 wow fadeIn" data-wow-delay="0.1s">
                    <%= simple_form_for @contact_form, input_html: { class: 'contact' } do |f| %>
                        <div class="form-group"> <span>Nome completo</span>
                            <%= f.input :name, label: false %>
                        </div>

                        <div class="form-group"> <span>Seu e-mail</span>
                            <%= f.input :email, label: false %>
                        </div>


                        <div class="form-group"> <span>Assunto</span>
                            <%= f.input :subject, label: false %>
                        </div>

                        <div class="form-group"> <span>Descrição da Mensagem</span>
                            <%= f.input :description, label: false %>
                        </div>

                        <div class="form-group">
                            <button id="submit" type="submit" name="submit">ENVIAR</button>
                        </div>
                    <% end %>

                    
                    <!-- end form-group -->
                </div>
                <!-- end col-6 -->
            </div>
            <!-- end row --> 
        </div>
        <!-- end container --> 
    </section>
    <!-- end contact -->

    <%= render 'layouts/partials/footer' %>
    <!-- end sub-footer --> 
</main>
<!-- end main -->

